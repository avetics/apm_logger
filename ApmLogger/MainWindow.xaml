<Window x:Class="ApmLogger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApmLogger"
        mc:Ignorable="d"
        Title="DataLogger" Height="540.4" Width="503">
    <Grid Margin="0,0,5.4,-16.8">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="22*"/>
            <ColumnDefinition Width="409*"/>
            <ColumnDefinition Width="84*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="stopBtn" Content="Stop " HorizontalAlignment="Left" Margin="378.8,461,0,0" VerticalAlignment="Top" Width="88"  Height="29" Click="cancel_button_Click" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label x:Name="label" Content="APM " HorizontalAlignment="Left" Margin="7.8,460,0,0" VerticalAlignment="Top" Height="30" Width="35" FontSize="10.667" Grid.Column="1"/>
        <Button x:Name="startBtn" Content="Start" HorizontalAlignment="Left" Margin="292.8,461,0,0" VerticalAlignment="Top" Width="81" Click="button_Click" Height="29" Grid.Column="1"/>
        <Label x:Name="statusText" Content="" Margin="21,416,7.8,0" VerticalAlignment="Top" Width="464" Height="42" HorizontalAlignment="Center" Grid.ColumnSpan="3" FontSize="16"/>
        <Label x:Name="label_Copy" Content="Hokuyu" HorizontalAlignment="Left" Margin="105.8,461,0,0" VerticalAlignment="Top" Height="30" Width="53" FontSize="10.667" Grid.Column="1"/>
        <Label x:Name="apmComLabel" Content="" HorizontalAlignment="Left" Margin="51.8,460,0,0" VerticalAlignment="Top" Height="30" Width="49" FontSize="10.667" Grid.Column="1"/>
        <Label x:Name="hokuyuComLabel" Content="" HorizontalAlignment="Left" Margin="158.8,461,0,0" VerticalAlignment="Top" Height="30" Width="44" FontSize="10.667" Grid.Column="1"/>
        <Button x:Name="button" Content="Refresh" HorizontalAlignment="Left" Margin="207.8,461,0,0" VerticalAlignment="Top" Width="80" Height="29" Click="button_Click_1" Grid.Column="1"/>
        <Button x:Name="zoom2" Content="-" HorizontalAlignment="Left" VerticalAlignment="Top" Width="34" Height="28" Click="button_Copy_Click" Grid.ColumnSpan="2" Margin="385.8,416,0,0" Grid.Column="1"/>
        <Button x:Name="zoom1" Content="+" HorizontalAlignment="Left" VerticalAlignment="Top" Width="34" Height="28" Click="button_Copy1_Click" Grid.Column="2" Margin="29.6,416,0,0"/>
        <Canvas x:Name="canvas" Height="292" Margin="0,10,31.8,0" VerticalAlignment="Top" Grid.ColumnSpan="3"/>
        <Label x:Name="label1" Content="Filesize   Hokuyu      Pitch            Yaw             UpLidar      DownLidar     " HorizontalAlignment="Left" Margin="13,395,0,0" VerticalAlignment="Top" Width="397" FontWeight="Bold" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
